<?xml version="1.0" encoding="utf-8" ?>
<CodeTemplate 
   xmlns="Degage.DataModel.Schema.Toolbox"
   xmlns:schemaLocation="Degage.DataModel.Schema.Toolbox CodeTemplate.xsd">
  <Name>.NET 简易模板-XML</Name>
  <Body>
    using System;
    namespace $NameSpace$
    {
          ///&lt;summary&gt;
          /// $TableExplain$
          ///&lt;/summary&gt;
          public class $ClassName$
          {
              #Properties#
          }
      }
  </Body>
  <ExtensionName>.cs</ExtensionName>
  <PropertyCodeTemplates>
    <Item>
      <Name>Properties</Name>
      <Content>
           ///&lt;summary&gt;
           /// $ColumnExplain$
           ///&lt;/summary&gt;
           public $MapType$ $PropertyName$ {get;set;}
      </Content>
    </Item>
  </PropertyCodeTemplates>
</CodeTemplate>
